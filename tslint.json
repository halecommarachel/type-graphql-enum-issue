{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
	    "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier": true,
        "no-implicit-dependencies": [true, "dev"],
        "no-submodule-imports": false,
        "ordered-imports": true,
        "no-console": false
    },
    "rulesDirectory": ["tslint-plugin-prettier"]
}
